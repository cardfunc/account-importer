{
 "name": "@cardfunc/account-importer",
 "version": "0.0.0",
 "description": "Script to import accounts into CardFunc.",
 "author": "CardFunc",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "git+https://github.com/cardfunc/account-importer"
 },
 "bugs": {
  "url": "https://github.com/cardfunc/account-importer/issues"
 },
 "homepage": "https://github.com/cardfunc/account-importer#readme",
 "private": false,
 "bin": {
  "cardfunc-account-importer": "dist/index.js"
 },
 "main": "dist/index.js",
 "typings": "dist/index.d.ts",
 "git": {
  "tagName": "v${version}"
 },
 "preferGlobal": true,
 "github": {
  "release": true
 },
 "jest": {
  "globals": {
   "ts-jest": {
    "tsConfig": "tsconfig.test.json"
   }
  },
  "testEnvironment": "node",
  "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
   "node_modules/",
   "dist/"
  ],
  "collectCoverageFrom": [
   "**/*.{ts,tsx,js,jsx}",
   "!**/node_modules/**",
   "!**/dist/**"
  ],
  "preset": "ts-jest",
  "testMatch": null
 },
 "scripts": {
  "dev": "watch 'npm run build' .",
  "lint": "tslint --config tslint.json --project .",
  "build": "tsc -p .",
  "test": "jest",
  "test:watch": "watch jest",
  "prepare": "npm run build",
  "clean": "rm -rf dist node_modules coverage"
 },
 "dependencies": {
  "@cardfunc/model": "0.0.45",
  "@cogneco/mend": "^1.7.15",
  "@typeup/dom": "^0.2.10",
  "@typeup/parser": "^0.2.10",
  "@typeup/renderer": "0.2.20",
  "commander": "^4.1.1"
 },
 "devDependencies": {
  "@types/jest": "^25.1.2",
  "@types/node": "^13.7.1",
  "babel-core": "^6.26.3",
  "babel-jest": "^25.1.0",
  "jest": "^25",
  "ts-jest": "^25.2.0",
  "tslint": "^6.0.0",
  "typescript": "^3.7.5",
  "typescript-tslint-plugin": "^0.5.5",
  "watch": "^1.0.2"
 }
}
